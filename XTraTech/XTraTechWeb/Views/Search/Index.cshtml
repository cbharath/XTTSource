                                        </div>
                                        <div class="col-md-2">
                                            <label>Infants</label>
                                            <div class="selector">
                                                @Html.DropDownListFor(m => m.InfantCount, Model.InfantList)
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Class of travel</label>
                                            <div class="selector">
                                                @Html.DropDownListFor(m => m.CabinClass, Model.CabinClassList)
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Preferred Airline</label>
                                            <div class="selector">
                                                <select>
                                                    <option value="">select name</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <hr>
                                    <button type="submit" class="btn full-width btn-medium sea-blue" onclick="return loader()">PROCEED TO RESULT</button>
                                </div>
                                }
