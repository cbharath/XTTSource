                                </div>
                                <div class="search-content">
                                    <div class="row choose-travel">
                                        <div class="col-xs-4">
                                            <label class="radio radio-inline">
                                                @Html.RadioButtonFor(m => m.JourneyType, "Return", new { @class = "choose-travel return", @checked = "checked" })
                                                @*<input type="radio" name="choose-travel" checked="checked" value="1">*@Round Trip
                                            </label>
                                        </div>
                                        <div class="col-xs-4">
                                            <label class="radio radio-inline">
                                                @Html.RadioButtonFor(m => m.JourneyType, "OneWay", new { @class = "choose-travel oneway" })
                                                @*<input type="radio" name="choose-travel" value="2">*@Oneway
                                            </label>
                                        </div>
                                        <div class="col-xs-4">
                                            <label class="radio radio-inline">
                                                @Html.RadioButtonFor(m => m.JourneyType, "MultiDestination", new { @class = "choose-travel multi" })
                                                @*<input type="radio" name="choose-travel" value="3">*@Multi-city
                                            </label>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="form-group row cloned">
                                        <div class="col-md-3">
                                            <label>Leaving From</label>
                                            @Html.TextBoxFor(m => m.CityPares[3].Origin, new { @class = "validate[required] input-text full-width", @placeholder = "enter your location", value = "", autocomplete = "off", data_autocomplete = Url.Action("AirportList", "Search") })
                                            @*<input type="text" class="input-text full-width" placeholder="enter your location" data-provide="typeahead" />*@
                                        </div>
                                        <div class="col-md-3">
                                            <label>Depart Date</label>
                                            <div class="datepicker-wrap dark-blue">
                                                @Html.TextBoxFor(m => m.CityPares[3].DepartureDateTime, new { @class = "validate[required] input-text full-width", @placeholder = "select date" })
                                                @*<input type="text" class="input-text full-width" placeholder="select date" />*@
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Going To</label>
                                            @Html.TextBoxFor(m => m.CityPares[3].Destination, new { @class = "validate[required] input-text full-width destination", @placeholder = "enter a destination", value = "", autocomplete = "off", data_autocomplete = Url.Action("AirportList", "Search") })
                                            @*<input type="text" class="input-text full-width" placeholder="enter a destination" />*@
                                        </div>
                                        <div class="col-md-3 toggler_one">
                                            <label>Return Date</label>
                                            <div class="datepicker-wrap dark-blue">
                                                @Html.TextBoxFor(m => m.CityPares[3].ReturnDateTime, new { @class = "validate[required] input-text full-width", @placeholder = "select date" })
                                                @*<input type="text" class="input-text full-width" placeholder="select date" />*@
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-3">
                                            <label>Leaving From</label>
                                            @Html.TextBoxFor(m => m.CityPares[0].Origin, new { @class = "validate[required] input-text full-width", @placeholder = "enter your location", value = "", autocomplete = "off", data_autocomplete = Url.Action("AirportList", "Search") })
                                            @*<input type="text" class="input-text full-width" placeholder="enter your location" data-provide="typeahead" />*@
                                        </div>
                                        <div class="col-md-3">
                                            <label>Depart Date</label>
                                            <div class="datepicker-wrap dark-blue">
                                                @Html.TextBoxFor(m => m.CityPares[0].DepartureDateTime, new { @class = "validate[required] input-text full-width", @placeholder = "select date" })
                                                @*<input type="text" class="input-text full-width" placeholder="select date" />*@
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <label>Going To</label>
                                            @Html.TextBoxFor(m => m.CityPares[0].Destination, new { @class = "validate[required] input-text full-width destination", @placeholder = "enter a destination", value = "", autocomplete = "off", data_autocomplete = Url.Action("AirportList", "Search") })
                                            @*<input type="text" class="input-text full-width" placeholder="enter a destination" />*@
                                        </div>
                                        <div class="col-md-3 toggler_one">
                                            <label>Return Date</label>
                                            <div class="datepicker-wrap dark-blue">
                                                @Html.TextBoxFor(m => m.CityPares[0].ReturnDateTime, new { @class = "validate[required] input-text full-width", @placeholder = "select date" })
                                                @*<input type="text" class="input-text full-width" placeholder="select date" />*@
                                            </div>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="form-group row">
                                        <div class="col-md-2">
                                            <label>Adults</label>
                                            <div class="selector">
                                                @Html.DropDownListFor(m => m.AdultCount, Model.AdultList)
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <label>Children</label>
                                            <div class="selector">
                                                @Html.DropDownListFor(m => m.ChildCount, Model.ChildList)
